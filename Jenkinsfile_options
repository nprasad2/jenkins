pipeline{
    agent any
    tools {
        maven 'Maven'
    }
    stages{
        stage('Checkout stage'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/nprasad2/addressbook.git']])
            }
        }
        stage('list files'){
            steps{
                sh 'cd Project'
                sh 'ls -l'
            }
        }
        stage('Build project'){
            steps{
                sh 'mvn compile'
            }
        }
    }
}