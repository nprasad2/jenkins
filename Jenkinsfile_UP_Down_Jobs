pipeline{
    agent any
    stages{
        stage('upstream'){
            steps{
                echo "This is Upstream job"
            }
        }
        stage('Downstream job'){
            steps{
                build job: 'python_proj'
            }

        }
        post {
           always {
    // One or more steps need to be included within each condition's block.
             }
            success {
            // One or more steps need to be included within each condition's block.
                }
              failure {
            // One or more steps need to be included within each condition's block.
        }
}

    }
}